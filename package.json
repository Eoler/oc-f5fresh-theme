{
  "name": "oc-f5fresh-theme",
  "version": "1.6.1",
  "description": "Starter development template for building Zurb Foundation 5 themed OctoberCMS sites.",
  "author": "Danilo CuculiÄ‡ <eoler@castus.me>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.0 <12.0"
  },
  "main": "gulpfile.js",
  "paths": {
    "root": "./",
    "media": "storage/media/**/",
    "assets": "assets/",
    "src": {
      "images": "images/**/",
      "styles": "scss/",
      "scripts": "es6/"
    },
    "dest": {
      "images": "images/",
      "styles": "css/",
      "scripts": "js/"
    },
    "ext": {
      "images": "*.{jpg,jpeg,png}",
      "styles": "*.scss",
      "scripts": "*.js"
    },
    "include": {
      "sass": [
        "assets/vendor/foundation/scss"
      ],
      "js": [
        "node_modules"
      ]
    },
    "sourcemap": {
      "root": "../scss"
    }
  },
  "images": {
    "maxwidth": "1280",
    "maxheight": "1280"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "iOS >= 9"
  ],
  "devDependencies": {
    "bower": "^1.8.8",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-if": "^3.0.0",
    "gulp-ignore": "^3.0.0",
    "gulp-include": "^2.4.1",
    "gulp-load-plugins": "^2.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-responsive": "^3.0.1",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "js-yaml": "^3.13.1",
    "pump": "^3.0.0",
    "yargs": "^15.1.0"
  },
  "scripts": {
    "start": "gulp watch",
    "build": "gulp upbuild --production"
  }
}
